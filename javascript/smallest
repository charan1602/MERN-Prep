let count1 = 0
let count = 0
let i=10
while( i>0){
    count =0
    for(let j=1 ;j<11;j++){
        if(i%j==0){
            count++
            // console.log(i)
            // console.log(count)
        }
       
    }
    //  console.log(i)
    if(count == 10){
        count1++
    }
    if( count1 == 4){
        // console.log(count1,i)
        //     if(count1==3){
              console.log(i)
              break
            // }

    }
    else{
        i = i+10
    }
}